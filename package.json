{
  "name": "rollup-module-boilerplate",
  "version": "0.1.0",
  "description": "Rollup Module Boilerplate",
  "main": "dist/rollup-module-boilerplate.cjs.js",
  "module": "dist/rollup-module-boilerplate.es.js",
  "unpkg": "dist/rollup-module-boilerplate.umd.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint *.js src/ test/",
    "lint:watch": "esw *.js src/ test/ -w",
    "coverage": "npm run ava:coverage",
    "ava": "ava --verbose",
    "ava:watch": "ava --watch --verbose",
    "ava:coverage": "nyc ava --verbose",
    "dev": "concurrently -p \"[{name}]\" -n \"lint,ava\" \"npm run lint:watch\" \"npm run ava:watch\"",
    "test": "npm run lint && npm run coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd-min",
    "build:cjs": "cross-env TARGET=cjs rollup -c",
    "build:es": "cross-env TARGET=es rollup -c",
    "build:umd": "cross-env TARGET=umd rollup -c",
    "build:umd-min": "cross-env TARGET=umd NODE_ENV=production rollup -c"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "babel": {
      "extensions": [
        "js"
      ]
    },
    "require": [
      "@babel/register"
    ],
    "failWithoutAssertions": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malash/rollup-module-boilerplate.git"
  },
  "keywords": [
    "rollup",
    "boilerplate"
  ],
  "author": "i@malash.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malash/rollup-module-boilerplate/issues"
  },
  "homepage": "https://github.com/malash/rollup-module-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-beta.8",
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "jsdom": "^12.0.0",
    "nyc": "^13.0.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^5.0.2"
  }
}
