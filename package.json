{
  "name": "rollup-module-boilerplate",
  "version": "0.1.0",
  "description": "Rollup Module Boilerplate",
  "main": "dist/rollup-module-boilerplate.cjs.js",
  "module": "dist/rollup-module-boilerplate.es.js",
  "unpkg": "dist/rollup-module-boilerplate.umd.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint *.js src/ test/ --ext .ts,.js",
    "lint:watch": "esw *.js src/ test/ --ext .ts,.js -w",
    "coverage": "npm run ava:coverage",
    "ava": "ava --verbose",
    "ava:watch": "ava --watch --verbose",
    "ava:coverage": "nyc ava --verbose",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch --preserveWatchOutput",
    "dev": "concurrently -p \"[{name}]\" -n \"lint,type-check,ava\" \"npm run lint:watch\" \"npm run type-check:watch\" \"npm run ava:watch\"",
    "test": "npm run lint && npm run type-check && npm run coverage",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd-min",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cjs": "cross-env TARGET=cjs rollup -c",
    "build:es": "cross-env TARGET=es rollup -c",
    "build:umd": "cross-env TARGET=umd rollup -c",
    "build:umd-min": "cross-env TARGET=umd NODE_ENV=production rollup -c"
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "sources": [
      "**/*.ts",
      "!dist/**/*"
    ],
    "compileEnhancements": false,
    "babel": {
      "extensions": [
        "ts"
      ]
    },
    "require": [
      "ts-node/register/transpile-only",
      "@babel/register"
    ],
    "failWithoutAssertions": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malash/rollup-module-boilerplate.git"
  },
  "keywords": [
    "rollup",
    "boilerplate"
  ],
  "author": "i@malash.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malash/rollup-module-boilerplate/issues"
  },
  "homepage": "https://github.com/malash/rollup-module-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-rc.1",
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.13.0",
    "eslint-watch": "^4.0.2",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^5.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^20.1.1"
  }
}
